{
    "name": "adamcmoore/laravel-location-validator",
    "description": "A Laravel validation rule to check that a lng/lat pair are within an area.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Moore",
            "email": "adam@acmoore.co.uk"
        }
    ],
    "keywords": [
        "laravel",
        "validator",
        "location"
    ],
    "require": {
        "php": ">=7.0.13",
        "illuminate/console": "5.6.*|5.7.*|5.8.*|^6.0",
        "illuminate/database": "5.6.*|5.7.*|5.8.*|^6.0",
        "illuminate/filesystem": "5.6.*|5.7.*|5.8.*|^6.0",
        "league/geotools": "^0.8"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": ">=3.5",
        "orchestra/database": ">=3.5"
    },
    "autoload": {
        "psr-4": {
            "AcMoore\\LocationValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcMoore\\LocationValidator\\Tests\\": "tests/"
        }
    }
}
