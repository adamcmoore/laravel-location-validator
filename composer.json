{
    "name": "adamcmoore/laravel-location-validator",
    "description": "A Laravel validation rule to check that a lng/lat pair are within an area.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Moore",
            "email": "adam@acmoore.co.uk"
        }
    ],
    "keywords": [
        "laravel",
        "validator",
        "location"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10",
        "illuminate/database": "^10",
        "illuminate/filesystem": "^10",
        "league/geotools": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "AcMoore\\LocationValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcMoore\\LocationValidator\\Tests\\": "tests/"
        }
    }
}
